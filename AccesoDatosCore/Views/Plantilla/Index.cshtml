@model List<Plantilla>
<h1>Plantilla </h1>

<table class="table table-dark">
    <thead>
        <tr>
            <th>Apellido</th>
            <th>Funcion</th>
            <th>Salario</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Plantilla pla in Model)
        {
            <tr>
                <td>@pla.Apellido</td>
                <td>@pla.Funcion</td>
                <td>@pla.Salario</td>
            </tr>
        }
    </tbody>
</table>